{
  "aggregator": {
    "interval_seconds": 60,
    "sources": [
      {
        "url": "https://raw.githubusercontent.com/TheSpeedX/PROXY-List/master/http.txt",
        "type": "txt",
        "protocol": "http",
        "enabled": true
      },
      {
        "url": "https://raw.githubusercontent.com/clarketm/proxy-list/master/proxy-list-raw.txt",
        "type": "txt",
        "protocol": "http",
        "enabled": true
      },
      {
        "url": "https://api.proxyscrape.com/v2/?request=get&protocol=http&timeout=10000&country=all",
        "type": "txt",
        "protocol": "http",
        "enabled": true
      },
      {
        "url": "https://www.proxy-list.download/api/v1/get?type=http",
        "type": "txt",
        "protocol": "http",
        "enabled": true
      },
      {
        "url": "https://api.proxyscrape.com/v2/?request=get&protocol=socks4&timeout=10000&country=all",
        "type": "txt",
        "protocol": "socks4",
        "enabled": true
      },
      {
        "url": "https://api.proxyscrape.com/v2/?request=get&protocol=socks5&timeout=10000&country=all",
        "type": "txt",
        "protocol": "socks5",
        "enabled": true
      },
      {
        "url": "https://raw.githubusercontent.com/TheSpeedX/PROXY-List/master/socks4.txt",
        "type": "txt",
        "protocol": "socks4",
        "enabled": true
      },
      {
        "url": "https://raw.githubusercontent.com/TheSpeedX/PROXY-List/master/socks5.txt",
        "type": "txt",
        "protocol": "socks5",
        "enabled": true
      }
    ],
    "user_agent": "Mozilla/5.0 (compatible; ProxyChecker/1.0)"
  },
  "zmap": {
    "enabled": true,
    "ports": [8080, 80, 3128],
    "rate_limit": 7000,
    "bandwidth": "",
    "max_runtime_seconds": 600,
    "target_ranges": [],
    "blacklist": [],
    "interface": "",
    "zmap_binary": "/usr/sbin/zmap",
    "output_format": "csv",
    "zmap_extra_args": [],
    "cooldown_seconds": 1800,
    "comment": "Zmap runs for 10 min (600s), finds ~4-5k IPs, repeats every 30 min (1800s). Optimized for continuous fresh proxies!"
  },
  "checker": {
    "timeout_ms": 10000,
    "concurrency_total": 20000,
    "batch_size": 2000,
    "retries": 0,
    "test_url": "http://www.gstatic.com/generate_204",
    "mode": "full-http",
    "enable_adaptive_concurrency": true,
    "max_fd_usage_percent": 70,
    "max_cpu_usage_percent": 75,
    "enable_fast_filter": true,
    "fast_filter_timeout_ms": 2000,
    "fast_filter_concurrency": 50000,
    "socks_enabled": true,
    "socks_timeout_ms": 15000,
    "socks_test_url": "http://www.gstatic.com/generate_204"
  },
  "api": {
    "addr": ":8083",
    "api_key_env": "PROXY_API_KEY",
    "rate_limit_per_minute": 1200,
    "rate_limit_per_ip": 100,
    "enable_api_key_auth": true,
    "enable_ip_rate_limit": true
  },
  "storage": {
    "type": "file",
    "path": "/data/proxies.json",
    "persist_interval_seconds": 300
  },
  "metrics": {
    "enabled": true,
    "endpoint": "/metrics",
    "namespace": "proxychecker"
  },
  "logging": {
    "level": "info",
    "format": "json"
  }
}

